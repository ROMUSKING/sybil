{
    "name": "sybil-ai-coding-agent",
    "displayName": "Sybil AI Coding Agent",
    "description": "A polyagentic AI coding assistant for VS Code, providing automated software development capabilities",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "AI",
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onCommand:sybil.startTask",
        "onCommand:sybil.resumeSession",
        "onCommand:sybil.clearSession"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "sybil.startTask",
                "title": "Sybil: Start New Task",
                "category": "Sybil"
            },
            {
                "command": "sybil.resumeSession",
                "title": "Sybil: Resume Session",
                "category": "Sybil"
            },
            {
                "command": "sybil.clearSession",
                "title": "Sybil: Clear Session",
                "category": "Sybil"
            },
            {
                "command": "sybil.showAnalytics",
                "title": "Sybil: Show Analytics",
                "category": "Sybil"
            }
        ],
        "configuration": {
            "title": "Sybil AI Coding Agent",
            "properties": {
                "sybil.apiKeys": {
                    "type": "object",
                    "description": "API keys for LLM providers"
                },
                "sybil.models": {
                    "type": "object",
                    "description": "Model configurations"
                },
                "sybil.agentModels": {
                    "type": "object",
                    "description": "Agent model assignments"
                },
                "sybil.pythonPath": {
                    "type": "string",
                    "description": "Path to Python executable for backend"
                },
                "sybil.verbose": {
                    "type": "boolean",
                    "description": "Enable verbose logging"
                }
            }
        },
        "views": {
            "explorer": [
                {
                    "id": "sybilSessions",
                    "name": "Sybil Sessions",
                    "icon": "$(robot)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "sybil.startTask",
                    "when": "view == sybilSessions",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "16.x",
        "eslint": "^8.28.0",
        "typescript": "^4.9.4",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0"
    },
    "dependencies": {
        "vscode-languageclient": "^8.1.0"
    }
}